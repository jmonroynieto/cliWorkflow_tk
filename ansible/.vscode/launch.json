    "version": "0.2.0",
    "configurations": [

        {
            "name": "colorless",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": ["this is my message", "-l", "warn"]
        },
        {
            "name": "no message",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}"
        },
        {
            "name": "unlock",
            "type" : "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": ["unlock"] 
        },
        {
            "name": "linesfeed",
            "type" : "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": ["<", "${workspaceFolder}/go.mod"],
            "console": "integratedTerminal",
        }
    ]
}